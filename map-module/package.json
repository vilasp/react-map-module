{
  "name": "map-module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-rewire": "^1.2.0",
    "notistack": "^0.9.0",
    "ol": "^5.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.0",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "build-flow-files": "babel src/ -d lib/",
    "prepublish": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-flow": "^7.0.0",
    "@testing-library/react": "^9.1.3",
    "flow-bin": "^0.106.3",
    "jest-canvas-mock": "^2.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "moduleNameMapper": {
      "^(Controllers|Api|Utilities)/(.*)$": "<rootDir>Scripts/$1/$2"
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "**/Scripts/{App,Controllers,Utilities,Localization,EntryPoints}/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
